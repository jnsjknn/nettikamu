{
  "name": "nettikamu",
  "version": "0.1.0",
  "description": "A platform for Finnish people to find friends online",
  "private": true,
  "engines": {
    "node": "15.6.0",
    "npm": "7.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-adsense": "^0.1.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "server": "cd server && npm run dev",
    "client": "react-scripts start",
    "build": "react-app-rewired build && (cd server && npm install)",
    "start": "cd server && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnsjknn/nettikamu.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Joonas Jokinen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnsjknn/nettikamu/issues"
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.8"
  },
  "cacheDirectories": [
    "server/node_modules",
    "node_modules"
  ]
}
